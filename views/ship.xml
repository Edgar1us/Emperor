<odoo>
    <data>
        <record model="ir.ui.view" id="emperor.ship_tree">
            <field name="name">Ship</field>
            <field name="model">emperor.ship</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
		            <field name="type_ship"/>
                    <field name="king"/>
                    <field name="kingdom"/>
                </tree>
            </field>
        </record>
        <record model="ir.ui.view" id="emperor.ship_form">
            <field name="name">Ship</field>
            <field name="model">emperor.ship</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="name"/>
			                <field name="type_ship"/>
                            <field name="king"/>
                            <field name="kingdom"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
    </data>
</odoo>
